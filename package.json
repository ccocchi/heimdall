{
  "name": "heimdall_apm",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@nivo/bar": "^0.42.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-scripts": "1.1.4"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/css/scss -o src/css/dist",
    "watch-css": "node-sass-chokidar src/css/scss -o src/css/dist --watch",
    "start-js": "react-scripts start",
    "start": "npm-run-all watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "node-sass-chokidar": "^1.3.0",
    "npm-run-all": "^4.1.3"
  }
}
